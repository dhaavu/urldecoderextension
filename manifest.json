{
    "manifest_version": 2,
  
    "name": "URL decoder",
    "description": "Decode Short URL from websites like Twitter, Youtube or any bit.ly URLs.",
    "version": "1.0.0",
    "icons": { "128": "icon.png" },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "tabs", "https://urldecode.herokuapp.com/"], 
    "author" : "Dhaval Patel"
  }